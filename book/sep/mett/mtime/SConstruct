from rsfproj import *

#x_shot=5200 
#mode=0
#n_theta=256
#dxo=25 nxo=256
#dzo=12.5 nzo=241
#dt=.008
#nf=24 
#fmax=60
#fmin=10

Fetch('marmvel.hh','marm')

Flow('marm','marmvel.hh',
     '''
     dd form=native | 
     smooth rect1=5 rect2=5 | 
     window min2=2000 j1=2 j2=2 n2=800 n1=376
     ''')

Plot('marm','window j1=3 j2=3 | grey allpos=y wantaxis=n wanttitle=n')

Flow('ett','marm','eikonal yshot=5200')

Plot('ett',
     '''
     contour nc=20 c0=.1 dc=.1 plotfat=4 label1=Depth label2=Distance
     title="Eikonal FD Traveltimes"
     ''')

Plot('marm-ett','marm ett','Overlay')

Result('marm-ett','marm ett','Overlay')

End()
