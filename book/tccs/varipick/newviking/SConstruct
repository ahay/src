Result(name+'-cmps','cmps',
   '''
   window min3=%g max3=%g max1=%g|
   byte gainpanel=e | 
   grey3 title="Viking Graben CMP Gathers" 
   frame1=0 unit2= label2=Trace frame2=%i frame3=%i
   point1=.9 point2=0.5 flat=n framelabel1=n
   '''%(min3,max3,max1tp,nh/4,frame3))

