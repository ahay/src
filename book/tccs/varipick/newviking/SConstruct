#       '''
#       window n2=1 | 
#       math output="(3.2-1.4)/(%s-1)/%s*(x1-%s)+%s-.1"|
#       clip2 lower=%s 
#       '''%(getstring2('n1'),getstring2('d1'),
#            getstring2('o1'),getstring2('o2'),getstring2('o2')))
name  = 'viking'
# sampling
nt = 1001
dt = 0.004
to = 0.
